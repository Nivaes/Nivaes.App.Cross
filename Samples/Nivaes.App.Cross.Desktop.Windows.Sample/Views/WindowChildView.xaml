<views:MvxWpfView
    x:Class="Nivaes.App.Cross.Desktop.Windows.Wpf.Sample.WindowChildView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Nivaes.App.Cross.Mobile.Sample.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=Nivaes.App.Cross.Windows.Wpf"
    xmlns:vm="clr-namespace:Nivaes.App.Cross.Sample.ViewModels;assembly=Nivaes.App.Cross.Sample"
    d:DesignHeight="300"
    d:DesignWidth="300"
    x:TypeArguments="vm:WindowChildViewModel"
    mc:Ignorable="d">
    <StackPanel Background="LightGray">
        <TextBlock Margin="5" Text="{Binding Text}" />
    </StackPanel>
</views:MvxWpfView>
