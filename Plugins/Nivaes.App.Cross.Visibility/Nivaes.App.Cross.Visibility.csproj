<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net5.0</TargetFrameworks>
    <AssemblyName>Nivaes.App.Cross.Visibility</AssemblyName>
    <RootNamespace>Nivaes.App.Cross.Visibility</RootNamespace>
    <Description>
      Nivaes.App.Cross is the .NET framework for cross-platform solutions, including Xamarin iOS, Xamarin Android, Windows and Mac.

      This package contains the 'Visibility' plugin for Nivaes.App.Cross
    </Description>
    <PackageId>Nivaes.App.Cross.Visibility</PackageId>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>MvvmCross.Plugin.Visibility</AssemblyName>
    <RootNamespace>MvvmCross.Plugin.Visibility</RootNamespace>
    <Description>MvvmCross is the .NET MVVM framework for cross-platform solutions, including Xamarin iOS, Xamarin Android, Xamarin Forms, Windows and Mac.

This package contains the 'Visibility' plugin for MvvmCross</Description>
    <PackageId>MvvmCross.Plugin.Visibility</PackageId>
    <!--<ProduceReferenceAssembly>true</ProduceReferenceAssembly>-->
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Nivaes.App.Cross\Nivaes.App.Cross.csproj" />
  </ItemGroup>

</Project>